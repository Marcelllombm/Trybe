//Exerc√≠cio 2: Utilizando o modificador $each , 
//adicione as categorias "villain" e "comic-based" ao filme Batman .
use('filmes');

db.movies.updateOne(
  {
    title: "Batman"
  },
  {
    $push: 
    {
      category: 
      {
        $each: 
        [
          "villain", "comic-based"
        ],
      },
    },
  },
);

db.movies.find().pretty();